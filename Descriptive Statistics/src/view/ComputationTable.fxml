<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="controller.ComputationTableController">
   <children>
      <TableView fx:id="groupedData" layoutX="28.0" layoutY="21.0" prefHeight="425.0" prefWidth="544.0">
        <columns>
          <TableColumn fx:id="lowerClassLimit" prefWidth="75.0" text="Lower Class Limit" />
          <TableColumn fx:id="upperClassLimit" prefWidth="75.0" text="Upper Class Limit" />
            <TableColumn fx:id="frequency" prefWidth="75.0" text="Frequency" />
            <TableColumn fx:id="classMarks" prefWidth="75.0" text="Class Marks" />
            <TableColumn fx:id="fixi" prefWidth="75.0" text="FiXi" />
            <TableColumn fx:id="fixi2" prefWidth="75.0" text="FiXi^2" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnContinue" layoutX="268.0" layoutY="461.0" mnemonicParsing="false" onAction="#continueClick" text="Continue" />
   </children>
</AnchorPane>
