<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="130.0" prefWidth="250.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="controller.EditController">
   <children>
      <Button layoutX="172.0" layoutY="24.0" mnemonicParsing="false" onAction="#editClick" prefHeight="54.0" prefWidth="52.0" text="Edit" />
      <TextField fx:id="indexField" layoutX="14.0" layoutY="24.0" promptText="Index" />
      <TextField fx:id="valueField" layoutX="14.0" layoutY="54.0" promptText="Value" />
      <Button fx:id="btnConfirm" layoutX="96.0" layoutY="91.0" mnemonicParsing="false" onAction="#confirmClick" text="Confirm" />
   </children>
</AnchorPane>
