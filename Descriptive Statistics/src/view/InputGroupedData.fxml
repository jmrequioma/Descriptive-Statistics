<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="controller.InputGroupedDataController">
   <children>
      <TableView fx:id="groupedData" editable="true" layoutX="13.0" layoutY="58.0" prefHeight="285.0" prefWidth="574.0">
        <columns>
          <TableColumn fx:id="lowerClassLimit" prefWidth="75.0" text="Lower Class Limit" />
          <TableColumn fx:id="upperClassLimit" prefWidth="75.0" text="Upper Class Limit" />
            <TableColumn fx:id="frequency" prefWidth="75.0" text="Frequency" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="247.0" layoutY="25.0" prefHeight="17.0" prefWidth="111.0" text="Input Grouped Data" />
      <Button fx:id="continueBtn" disable="true" layoutX="270.0" layoutY="461.0" mnemonicParsing="false" onAction="#continueClick" text="Continue" />
      <ComboBox fx:id="classInterval" layoutX="126.0" layoutY="360.0" onAction="#intervalComboxAction" prefHeight="25.0" prefWidth="91.0" />
      <Label layoutX="44.0" layoutY="364.0" text="Class Interval" />
      <Label layoutX="287.0" layoutY="364.0" text="Lower Class Limit" />
      <TextField fx:id="lowerClassLimitTxtF" layoutX="383.0" layoutY="360.0" prefHeight="25.0" prefWidth="50.0" />
      <Label layoutX="284.0" layoutY="397.0" text="Upper Class Limit" />
      <TextField fx:id="upperClassLimitTxtF" layoutX="383.0" layoutY="393.0" prefHeight="25.0" prefWidth="50.0" />
      <Label layoutX="71.0" layoutY="397.0" text="Frequency" />
      <TextField fx:id="frequencyTxtF" layoutX="167.0" layoutY="393.0" prefHeight="25.0" prefWidth="50.0" />
      <Button layoutX="494.0" layoutY="375.0" mnemonicParsing="false" onAction="#editClick" text="Edit" />
   </children>
</AnchorPane>
